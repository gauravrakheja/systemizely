<%= form_with model: house, html: { class: form_inline(inline) } do |form| %>
  <div class="form-group mr-2">
    <%= form.text_field :name, placeholder: "House Name", class: "form-control" %>
  </div>
  <%= form.hidden_field :owner_id, value: current_user.id %>
  <%= form.submit "Create a new house", class: "btn btn-dark" %>
<% end %>